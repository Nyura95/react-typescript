{
  "name": "react-typescript",
  "version": "1.5.0",
  "description": "example reactJS with typescript and redux async",
  "main": "./src/index.js",
  "scripts": {
    "dev": "set NODE_ENV=development &&parcel ./src/index.html --open --no-cache --no-hmr",
    "start": "set NODE_ENV=development &&parcel ./src/index.html --open",
    "build": "set NODE_ENV=production &&parcel build ./src/index.html --out-dir build --detailed-report",
    "serve": "node server.js 3000",
    "watch": "parcel watch ./src/index.html",
    "tslint": "tslint --project tsconfig.json ./src/**/*.tsx&&tslint --project tsconfig.json ./src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nyura95/react-typescript.git"
  },
